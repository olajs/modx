{
  "name": "@olajs/modx",
  "version": "3.0.4",
  "description": "modx is a lightweight library to help developer use redux in a simple way",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "node ./scripts/build.js",
    "prepublish": "npm run build",
    "example": "npm run build && cp -r lib/ example/node_modules/@olajs/modx/lib && rm -rf example/node_modules/.cache/default-development",
    "release": "npm publish",
    "release:next": "npm publish --tag next"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css,less,scss,sass,md,json,html}": "prettier --write"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/olajs/modx.git"
  },
  "keywords": [
    "react",
    "redux",
    "library"
  ],
  "author": "richardchen85",
  "license": "MIT",
  "dependencies": {
    "react-redux": "^7.0.0",
    "redux": "^4.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.8",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@types/node": "^17.0.22",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.7",
    "prettier": "^2.6.0",
    "react": "^17.0.2",
    "rimraf": "^3.0.2",
    "typescript": "^4.6.2"
  },
  "peerDependencies": {
    "react": ">=16.8"
  },
  "files": [
    "lib"
  ],
  "bugs": {
    "url": "https://github.com/olajs/modx/issues"
  },
  "homepage": "https://github.com/olajs/modx#readme"
}
